{
  "name": "bouncer",
  "version": "0.14.1",
  "type": "module",
  "description": "Ridiculously configurable website limiter.",
  "main": "manifest.json",
  "scripts": {
    "test": "jest",
    "dev": "run-p -rl dev:*",
    "dev:scripts": "rollup --config --watch",
    "dev:ui": "eleventy --watch --quiet",
    "dev:webext": "web-ext run",
    "build": "run-s -l build:scripts build:ui",
    "build:scripts": "rollup --config",
    "build:ui": "eleventy",
    "build:webext": "web-ext build",
    "package": "run-s -l build:scripts build:ui build:webext"
  },
  "author": "Noah Pauls <noah@noahpauls.com>",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@jest/globals": "^29.4.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/luxon": "^3.3.0",
    "@types/webextension-polyfill": "^0.10.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.7.0",
    "luxon": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.16.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "web-ext": "^7.5.0",
    "webextension-polyfill": "^0.10.0"
  }
}
